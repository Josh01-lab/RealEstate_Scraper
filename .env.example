ENV=dev            # dev | prod

# local sqlite (used for local testing only)
SQLITE_DB=./data/db/central.db

# supabase (used in prod/CI; never commit real values)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# scraper knobs
MAX_PAGES=200
RATE_LIMIT_DELAY=1.0
SCRAPING_MODE=playwright   # requests | playwright | selenium
PORTALS_CONFIG=./config/portals.json

# logging
LOG_LEVEL=INFO
